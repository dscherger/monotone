#  -*- Autoconf -*-

AT_SETUP([importing a file])

MTN_SETUP

AT_DATA(importme, [version 0 of test file
])
AT_CHECK(CANONICALISE(importme))

TSHA=`SHA1(importme)`

AT_CHECK(MTN add importme, [], [ignore], [ignore])
AT_CHECK(MTN --branch=testbranch commit --message='blah blah', [], [ignore], [ignore])
AT_CHECK(MTN automate get_file $TSHA, [], [stdout])
AT_CHECK(CANONICALISE(stdout))
AT_CHECK(cmp importme stdout, [], [ignore])

AT_CLEANUP
