AT_SETUP([db missing])
MTN_SETUP

AT_CHECK(mkdir foo)

AT_DATA(foo/foo.db, [foo file
])

AT_CHECK( (cd foo && MTN --db= ls keys), [1], [ignore], [ignore])
AT_CHECK( (cd foo && MTN --db=foo.db ls keys), [1], [ignore], [ignore])
AT_CHECK( (cd foo && MTN --db=missing.db ls keys), [1], [ignore], [ignore])

AT_CLEANUP