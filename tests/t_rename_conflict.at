AT_SETUP([merge(<rename a b>, <rename a c>)])
MTN_SETUP

# Should cause a merge conflict.

ADD_FILE(a, [blah blah
])
COMMIT(testbranch)
BASE_R=`BASE_REVISION`

AT_CHECK(mv a b)
AT_CHECK(MTN rename a b, [], [ignore], [ignore])
COMMIT(testbranch)

AT_CHECK(mv b a)
REVERT_TO($BASE_R)

AT_CHECK(mv a c)
AT_CHECK(MTN rename a c, [], [ignore], [ignore])
COMMIT(testbranch)

AT_CHECK(MTN merge --branch=testbranch, [1], [ignore], [ignore])

AT_CLEANUP