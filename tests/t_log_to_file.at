# -*- Autoconf -*-

AT_SETUP([check --log])

MTN_SETUP

AT_DATA(input.txt, [random content
])

AT_CHECK(MTN add input.txt, [], [ignore], [ignore])

AT_CHECK(MTN --branch=testbranch --log=log.log commit -m "test", [], [stdout], [ignore])

AT_CHECK(QGREP('^mtn:' stdout), [1])
AT_CHECK(QGREP('^mtn:' log.log), [0])

AT_CLEANUP
