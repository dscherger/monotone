AT_SETUP([persistent netsync server - keys])
AT_KEYWORDS([netsync])

MTN_SETUP
NETSYNC_SETUP

AT_DATA(testfile, [blah balh
balh
])
AT_CHECK(MTN2 add testfile, [], [ignore], [ignore])
AT_CHECK(MTN2 commit --branch=testbranch --message foo, [], [ignore], [ignore])

AT_CHECK((echo foo@foo ; echo foo@foo) | MTN2 genkey foo@foo, [], [ignore], [ignore])

NETSYNC_SERVE_START(testbranch)

NETSYNC_CLIENT_N_RUN(2, push, testbranch)
NETSYNC_CLIENT_N_RUN(3, pull, testbranch)

AT_CHECK(MTN3 ls keys, [], [stdout], [ignore])
AT_CHECK(QGREP(foo@foo stdout), [1])

AT_DATA(testfile, [stuffty stuffty
])
AT_CHECK(MTN2 commit --branch=testbranch --message foo --key=foo@foo, [], [ignore], [ignore])

NETSYNC_CLIENT_N_RUN(2, push, testbranch)
NETSYNC_CLIENT_N_RUN(3, pull, testbranch)

NETSYNC_SERVE_STOP

AT_CHECK(MTN3 ls keys, [], [stdout], [ignore])
AT_CHECK(QGREP(foo@foo stdout))

AT_CLEANUP
