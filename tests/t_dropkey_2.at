# -*- Autoconf -*-

AT_SETUP([drop a public and private key])

MTN_SETUP

AT_CHECK((echo john@doe.com; echo john@doe.com) | MTN genkey john@doe.com, [], [ignore], [ignore])

AT_CHECK(MTN privkey john@doe.com, [], [stdout], [ignore])

AT_CHECK(QGREP('john@doe.com' stdout), [])

AT_CHECK(MTN dropkey john@doe.com, [], [ignore], [ignore])

AT_CHECK(MTN privkey john@doe.com, [1], [ignore], [stderr])

AT_CHECK(QGREP('do not exist' stderr), [])

AT_CHECK(MTN pubkey john@doe.com, [1], [ignore], [stderr])

AT_CHECK(QGREP('does not exist' stderr), [])

AT_CLEANUP
